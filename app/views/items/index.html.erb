<div id="heading" class="">
  <h1>Items</h1>
</div>
<div class="row">

  <div class="items">
    <% @items.each do |item|  %>
      <div class="col m4" id="item_<%= item.id %>">
        <h5><%= item.name %></h5>
        <%= image_tag(item.image_url) %>
        <p><%= item.description %></p>
        <p>Price per day: <strong><%= item.price %></strong></p>
      </div>
    <% end %>
  </div>
</div>
